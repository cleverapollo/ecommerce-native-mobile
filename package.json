{
  "name": "wantic",
  "version": "1.6.0",
  "author": "Tim Fischer",
  "homepage": "https://www.wantic.io",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --configuration=dev",
    "app:beta:web": "ionic serve --configuration=beta",
    "app:beta:ios": "ionic capacitor build ios --configuration=beta && npx cap copy ios && npx cap open ios",
    "app:beta:android": "ionic capacitor build android --configuration=beta && npx cap copy android && npx cap open android",
    "app:prod:and": "npx cap sync android && ionic capacitor run android --configuration=production",
    "app:prod:ios": "npm run build && npx cap copy ios &&  npx cap open ios",
    "build": "ionic build --prod",
    "build:beta": "ionic build --configuration=beta",
    "deploy:web:beta": "ionic build --configuration=beta && firebase deploy -P default",
    "deploy:web:prod": "ionic build --prod && firebase deploy -P prod",
    "test": "ng test --code-coverage",
    "lint": "npx ng lint",
    "lint:fix": "npx ng lint --fix",
    "mock:server": "nodemon server/server.js --ext js,ts,json.html",
    "e2e": "ng e2e --configuration=dev",
    "ci:build": "ionic build --configuration=beta",
    "ci:test": "ng test --code-coverage --no-watch --no-progress --browsers ChromeHeadless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.2.10",
    "@angular/core": "^15.2.10",
