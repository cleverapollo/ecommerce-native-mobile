{
  "name": "wantic",
  "version": "1.5.6",
  "author": "Tim Fischer",
  "homepage": "https://www.wantic.io",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --configuration=dev",
    "beta-web": "ionic serve --configuration=beta",
    "dev-beta-ios": "npx cap sync && ionic capacitor run ios -l --external --configuration=beta",
    "dev-beta-android": "npx cap sync android && ionic capacitor run android -l --external --configuration=beta --public-host=192.168.0.251",
    "dev-prod-ios": "npx cap sync && ionic capacitor run ios -l --external --configuration=production",
    "dev-prod-android": "npx cap sync && ionic capacitor run android -l --external --configuration=production",
    "build-beta-ios": "ionic capacitor build ios --configuration=beta && npx cap copy ios && npx cap open ios",
    "build-beta-android": "ionic capacitor build android --configuration=beta && npx cap copy android && npx cap open android",
    "build-prod-and": "ionic capacitor build android --configuration=production && npx cap copy android && npx cap open android",
    "build-prod-ios": "npm run build && npx cap copy ios &&  npx cap open ios",
    "build-prod-ios-fix-update": "npm run build-prod-ios && cd ios/App/public && zip -r ../wantic.zip ./* -x \"*.DS_Store\"",
    "build": "ionic build --prod",
    "build-beta": "ionic build --configuration=beta",
    "deploy-web-beta": "ionic build --configuration=beta && firebase deploy -P default",
    "deploy-web-prod": "ionic build --prod && firebase deploy -P prod",
    "test": "ng test --code-coverage",
    "lint": "npx ng lint",
    "e2e": "ng e2e --configuration=dev",
    "ci:build": "ionic build --configuration=beta",
    "ci:test": "ng test --code-coverage --no-watch --no-progress --browsers ChromeHeadless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@angular/service-worker": "^12.2.16",
    "@auth0/angular-jwt": "^5.0.2",
    "@awesome-cordova-plugins/core": "^5.46.0",
    "@awesome-cordova-plugins/http": "^5.46.0",
    "@awesome-cordova-plugins/in-app-browser": "^5.46.0",
    "@capacitor-firebase/analytics": "^5.1.0",
    "@capacitor-firebase/authentication": "^5.1.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/browser": "^5.0.6",
    "@capacitor/core": "^5.0.0",
    "@capacitor/device": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/preferences": "^5.0.0",
    "@capacitor/share": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ionic/angular": "^6.2.4",
    "@ionic/storage": "^2.3.1",
    "@ionic/storage-angular": "^3.0.6",
    "appsflyer-capacitor-plugin": "^6.5.2",
    "capacitor-secure-storage-plugin": "^0.5.1",
    "cordova-android": "10.1.1",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-advanced-http": "^3.3.1",
    "cordova-plugin-file": "^8.0.0",
    "cordova-plugin-inappbrowser": "5.0.0",
    "cordova-support-android-plugin": "^1.0.2",
    "core-js": "^3.32.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "es6-promise-plugin": "^4.2.2",
    "firebase": "^10.3.0",
    "globalthis": "^1.0.2",
    "ionic-cache": "^6.0.3",
    "ionic-native-http-connection-backend": "^10.0.0",
    "ngx-logger": "^4.3.3",
    "rxjs": "^6.6.7",
