{
  "name": "wantic",
  "version": "1.5.3",
  "author": "Tim Fischer",
  "homepage": "https://www.wantic.io",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --configuration=dev",
    "beta-web": "ionic serve --configuration=beta",
    "dev-beta-ios": "npx cap sync && ionic capacitor run ios -l --external --configuration=beta",
    "dev-beta-android": "npx cap sync android && ionic capacitor run android -l --external --configuration=beta --public-host=192.168.0.251",
    "dev-prod-ios": "npx cap sync && ionic capacitor run ios -l --external --configuration=production",
    "dev-prod-android": "npx cap sync && ionic capacitor run android -l --external --configuration=production",
    "build-beta-ios": "ionic capacitor build ios --configuration=beta && npx cap copy ios && npx cap open ios",
    "build-beta-android": "ionic capacitor build android --configuration=beta && npx cap copy android && npx cap open android",
    "build-prod-and": "npx cap sync android && ionic capacitor run android --configuration=production",
    "build-prod-ios": "npm run build && npx cap copy ios &&  npx cap open ios",
    "build-prod-ios-fix-update": "npm run build-prod-ios && cd ios/App/public && zip -r ../wantic.zip ./* -x \"*.DS_Store\"",
    "build": "ionic build --prod",
    "build-beta": "ionic build --configuration=beta",
    "deploy-web-beta": "ionic build --configuration=beta && firebase deploy -P default",
    "deploy-web-prod": "ionic build --prod && firebase deploy -P prod",
    "test": "ng test --code-coverage",
    "lint": "npx ng lint",
    "e2e": "ng e2e --configuration=dev",
    "postinstall": "jetifier",
    "ci:build": "ionic build --configuration=beta",
    "ci:test": "ng test --code-coverage --no-watch --no-progress --browsers ChromeHeadless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
