{
  "name": "wantic",
  "version": "1.0.0",
  "author": "Tim Fischer",
  "homepage": "https://www.wantic.io",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve --configuration=dev",
    "beta-web": "ionic serve --configuration=beta",
    "build-dev-ios": "npx cap sync && ionic capacitor run ios -l --external --configuration=beta",
    "build-beta-ios": "npx cap sync ios && ionic capacitor run ios --configuration=beta",
    "build-prod-ios": "npx cap sync ios && ionic capacitor run ios --configuration=production",
    "build-prod-ios-fix-update": "npm run build-prod-ios && cd ios/App/public && zip -r ../wantic.zip ./* -x \"*.DS_Store\"",
    "build": "ng build",
    "deploy-web-beta": "ionic build --configuration=beta && firebase deploy",
    "deploy-web-prod": "ionic build --prod && firebase deploy -P prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@angular/service-worker": "~9.1.12",
    "@auth0/angular-jwt": "^3.0.1",
    "@capacitor-community/firebase-analytics": "^0.2.0",
    "@capacitor/android": "^2.4.2",
    "@capacitor/core": "2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@ionic-native/core": "^5.28.0",
    "@ionic-native/file": "^5.29.0",
    "@ionic-native/http": "^5.28.0",
    "@ionic-native/in-app-browser": "^5.28.0",
    "@ionic/angular": "^5.3.2",
    "@ionic/storage": "^2.3.1",
    "capacitor-secure-storage-plugin": "^0.5.0",
    "cordova-android": "^8.1.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-advanced-http": "^3.0.1",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-inappbrowser": "^4.0.0",
    "cordova-plugin-ionic": "git+https://github.com/appit-online/appit-ionic-plugin.git",
    "cordova-sqlite-storage": "^5.1.0",
    "core-js": "^2.6.11",
    "es6-promise-plugin": "^4.2.2",
    "ionic-cache": "^5.2.0",
    "ionic-native-http-connection-backend": "^8.0.0",
    "ngx-logger": "^4.1.9",
    "rxjs": "^6.6.3",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.901.12",
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular-devkit/core": "~9.1.12",
    "@angular-devkit/schematics": "~9.1.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@capacitor/cli": "2.4.2",
    "@ionic/angular-toolkit": "^2.3.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.12.6",
    "codelyzer": "^5.2.2",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-file": {},
      "cordova-plugin-ionic": {
        "APP_ID": "17865333448040008fa8769dee6c5200",
        "CHANNEL_NAME": "Production",
        "UPDATE_METHOD": "background",
        "UPDATE_API": "https://build.appit-online.de",
        "WARN_DEBUG": "false",
        "MAX_STORE": "2",
        "MIN_BACKGROUND_DURATION": "30"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
